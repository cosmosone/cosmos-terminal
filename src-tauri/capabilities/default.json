{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for Cosmos Terminal",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "shell:default",
    "store:allow-load",
    "store:allow-get-store",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save"
  ]
}
